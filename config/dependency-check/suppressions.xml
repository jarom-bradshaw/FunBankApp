<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- Suppress false positives for Spring Boot dependencies -->
    <suppress>
        <notes>Spring Boot dependencies are generally safe</notes>
        <gav>org.springframework.boot:spring-boot-starter-*</gav>
        <cve>CVE-2023-24998</cve>
    </suppress>

    <!-- Suppress JWT library false positives -->
    <suppress>
        <notes>JWT library is used correctly</notes>
        <gav>io.jsonwebtoken:jjwt-*</gav>
        <cve>CVE-2023-24998</cve>
    </suppress>

    <!-- Suppress MySQL connector false positives -->
    <suppress>
        <notes>MySQL connector is used correctly</notes>
        <gav>com.mysql:mysql-connector-j</gav>
        <cve>CVE-2023-24998</cve>
    </suppress>

    <!-- Suppress H2 database false positives -->
    <suppress>
        <notes>H2 database is used for testing only</notes>
        <gav>com.h2database:h2</gav>
        <cve>CVE-2023-24998</cve>
    </suppress>

</suppressions> 