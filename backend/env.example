# === FunBankApp Environment Variables ===
# Copy this file to .env and fill in your actual values
# DO NOT commit .env file to version control

# === Database Configuration ===
DB_URL=jdbc:mysql://your-database-host:port/database-name?useSSL=true&requireSSL=true&verifyServerCertificate=false
DB_USERNAME=your-database-username
DB_PASSWORD=your-database-password
DB_DRIVER=com.mysql.cj.jdbc.Driver

# === JWT Configuration ===
JWT_SECRET=your-super-secure-jwt-secret-key-here
JWT_EXPIRATION=86400000

# === Server Configuration ===
SERVER_PORT=8080

# === Cookie Configuration (Production) ===
COOKIE_DOMAIN=your-domain.com
COOKIE_SECURE=true

# === Environment ===
# Set this to: dev, prod, or test
SPRING_PROFILES_ACTIVE=dev

# === CORS Configuration ===
CORS_ALLOWED_ORIGINS=https://your-production-domain.com,https://www.your-production-domain.com

# === Rate Limiting Configuration ===
RATE_LIMIT_AUTH_REQUESTS=5
RATE_LIMIT_AUTH_DURATION=60
RATE_LIMIT_GENERAL_REQUESTS=100
RATE_LIMIT_GENERAL_DURATION=60 